{
    "name": "boyhagemann/wiss",
    "description": "Wiss CMS",
    "license": "BSD-3-Clause",
	"minimum-stability": "dev",
	"repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "boyhagemann/ZfcTwitterBootstrap",
                "source": {
                    "type": "git",
                    "url": "https://github.com/boyhagemann/ZfcTwitterBootstrap",
                    "reference": "master"
                } 
            }

        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "boyhagemann/zf2-assetic-module",
                "source": {
                    "type": "git",
                    "url": "https://github.com/boyhagemann/zf2-assetic-module",
                    "reference": "master"
                } 
            }

        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-master",
		"doctrine/doctrine-module": "dev-master",
		"doctrine/doctrine-orm-module": "dev-master",
		"gedmo/doctrine-extensions": "dev-master",
		"boyhagemann/ZfcTwitterBootstrap": "dev-master",
		"boyhagemann/zf2-assetic-module": "dev-master",
        "kriswallsmith/assetic": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AsseticBundle": "vendor/boyhagemann/zf2-assetic-module/src/",
            "ZfcTwitterBootstrap": "vendor/boyhagemann/ZfcTwitterBootstrap/src/"
        }
    }
}
